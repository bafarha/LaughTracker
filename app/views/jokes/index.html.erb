<div class="main">
	<div class="container">
		<h1>My Jokes</h1>
		<% @jokes.each do |j| %>
			<div class="jokes col-lg-6">
				<h3><a href='<%= joke_path(j) %>'><%= j.title %></a></h3>
				<small><em><%= "#{time_ago_in_words(j.created_at)} ago" %></em></small>
				<!--<p><%= j.content %></p>-->
				<!--<%= link_to "See Joke", joke_path(j) %>-->
			</div>
		<% end %>
		<%= link_to "New Joke", new_joke_path(@joke) %>
	</div>
</div>

